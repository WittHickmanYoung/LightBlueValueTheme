			"settings": {
				"foreground": "#7F0000",
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#7F0000",
			}
		},
		{
			"scope": "support.other",
			"settings": {
				"foreground": "#7F0000",
			}
		},

		{
			"scope": "variable.language",
			"settings": {
